select content_id, sum(uv_num), sum(play_id_num), sum(share_id_num), sum(fav_id_num)
from (
	select content_id, count(distinct id) as uv_num, '0' as play_id_num, '0' as share_id_num, '0' as fav_id_num
	from muce_app_client.showevent where p_product= 'ANDROID_EYEPETIZER'  and p_date = 20160108 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id in (4036,3976,4650,4558,4058) group by content_id union all select content_id, '0' as uv_num, count(distinct id) as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_app_client.clickevent where p_product='ANDROID_EYEPETIZER'  and p_date = 20160108 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id in (4036,3976,4650,4558,4058) and click_name = 'Play' group by content_id union all select content_id, '0' as uv_num, '0' as play_id_num, count(distinct id) as share_id_num, '0' as fav_id_num from muce_app_client.clickevent where p_product = 'ANDROID_EYEPETIZER' and click_action = 'SHARE' and click_name in ('Moment','Wechat','QQ','Weibo','SystemShare') and p_date = 20160108 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id in (4036,3976,4650,4558,4058) group by content_id union all select content_id, '0' as uv_num, '0' as play_id_num, '0' as share_id_num, count(distinct id) as fav_id_num from muce_app_client.clickevent where p_product = 'ANDROID_EYEPETIZER' and click_name = 'Favorite' and p_date = 20160108 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id in (4036,3976,4650,4558,4058) group by content_id) a group by content_id --Android 20160108



select count(distinct case when p_product = 'IOS_EYEPETIZER' then id else null end), count(distinct case when p_product = 'ANDROID_EYEPETIZER' then id else null end)
from muce_topic.applicationstart
where p_date >= 20150623 and p_date <= 201507022


select p_date, p_hourmin, count(distinct case when p_product = 'IOS_EYEPETIZER' then id else null end), count(distinct case when p_product = 'ANDROID_EYEPETIZER' then id else null end)
from muce_app_client.applicationstartevent
where p_date >= 20151101 and p_date <= 20151130
group by p_date, p_hourmin

