select a.id
from (
	select distinct id
	from muce_topic.click
	where p_date = 20160125 and p_product = 'IOS_EYEPETIZER' and click_name like 'FeedPageEventClickhttp://wandou.im/1b3ze8') a join (
	select distinct id
	from muce_topic.click
	where p_date = 20160125 and p_product = 'IOS_EYEPETIZER' and click_action = 'SHARE' and click_name = 'Moment' ) b
on a.id = b.id


