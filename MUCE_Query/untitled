select p_date, client_first_source , count(distinct id)
from muce_topic.applicationstart
where p_date >= 20151207 and p_date <= 20151227 and client_first_source in ('eyepetizer_xiaomi_ad','eyepetizer_meizu_market')
group by p_date, client_first_source


select p_date, sum(case when p_product = 'IOS_EYEPETIZER' then 1 else 0 end), sum(case when p_product = 'ANDROID_EYEPETIZER' then 1 else 0 end)
from muce_topic.click
where p_date >= 20151207 and p_date <= 20151227 and click_name = 'Play'
group by p_date


select sum(case when flag_2 = 1 then 1 else 0 end), sum(case when flag_2 = 2 then 1 else 0 end), sum(case when flag_2 = 3 then 1 else 0 end)
from (
	select id, sum(flag) as flag_2
	from (
		select id, 1 as flag
		from muce_topic.applicationstart
		where p_product='ANDROID_EYEPETIZER' and is_start_new = 1 and p_date >= 20151223 and p_date <= 20151226 and client_first_source = 'eyepetizer_xiaomi_market'
		group by id
	union all
		select id, 2 as flag
		from muce_topic.applicationstart
		where p_product='ANDROID_EYEPETIZER' and p_date >= 20151224 and p_date <= 20151227
		group by id) a
	group by id) b
--ANDROID 新用户本周



select pp_date, sum(case when col_flag = 1 then col_num else 0 end), sum(case when col_flag = 2 then col_num else 0 end), sum(case when col_flag = 3 then col_num else 0 end)
from (
	select pp_date, col_flag, count(*) as col_num
		from
		(
			select pp_date, id, sum(col_flag) as col_flag 
			from 
			( 
					select concat(substring(p_date, 1, 4), '-', substring(p_date, 5, 2), '-', substring(p_date, 7, 2)) as pp_date, id, 1 as col_flag 
						from muce_topic.applicationstart
						where is_start_new = 1 and  p_date >= 20151223 and p_date <= 20151226 and p_product = 'ANDROID_EYEPETIZER' and client_first_source = 'eyepetizer_xiaomi_market'
					group by concat(substring(p_date, 1, 4), '-', substring(p_date, 5, 2), '-', substring(p_date, 7, 2)), id
				union all 
					select date_sub(concat(substring(p_date, 1, 4), '-', substring(p_date, 5, 2), '-', substring(p_date, 7, 2)), 1) as pp_date, id, 2 as col_flag 
						from muce_topic.applicationstart
						where p_date >= 20151224 and p_date <= 20151227 and p_product = 'ANDROID_EYEPETIZER'
					group by date_sub(concat(substring(p_date, 1, 4), '-', substring(p_date, 5, 2), '-', substring(p_date, 7, 2)), 1), id
			)ta 
			group by pp_date, id
		)tb
	group by pp_date, col_flag )tc
group by pp_date
--Android新用户次日活跃



select pp_date, device_os_version, sum(case when col_flag = 1 then 1 else 0 end), sum(case when col_flag = 2 then 1 else 0 end), sum(case when col_flag = 3 then 1 else 0 end)
from (
	select pp_date, device_os_version, id, sum(col_flag) as col_flag
	from (
			select concat(substring(a.p_date, 1, 4), '-', substring(a.p_date, 5, 2), '-', substring(a.p_date, 7, 2)) as pp_date, a.id, b.device_os_version, 1 as col_flag
			from (
				select p_date, id
				from muce_topic.applicationstart
				where p_date >= 20151101 and p_date <= 20151130 and p_product = 'ANDROID_EYEPETIZER' and is_start_new = 1
				group by p_date, id) a
			join (
				select p_date, id, device_os_version
				from muce_topic.applicationactive
				where p_date >= 20151101 and p_date <= 20151130 and p_product = 'ANDROID_EYEPETIZER') b
			on a.p_date = b.p_date and a.id = b.id
			group by concat(substring(a.p_date, 1, 4), '-', substring(a.p_date, 5, 2), '-', substring(a.p_date, 7, 2)), a.id, b.device_os_version
		union all
			select date_sub(concat(substring(c.p_date, 1, 4), '-', substring(c.p_date, 5, 2), '-', substring(c.p_date, 7, 2)), 1) as pp_date, c.id, d.device_os_version, 2 as col_flag
			from (
				select p_date, id
				from muce_topic.applicationstart
				where p_date >= 20151102 and p_date <= 20151201 and p_product = 'ANDROID_EYEPETIZER'
				group by p_date, id) c
			join (
				select p_date, id, device_os_version
				from muce_topic.applicationactive
				where p_date >= 20151102 and p_date <= 20151201 and p_product = 'ANDROID_EYEPETIZER') d
			on c.p_date = d.p_date and c.id = d.id
			group by date_sub(concat(substring(c.p_date, 1, 4), '-', substring(c.p_date, 5, 2), '-', substring(c.p_date, 7, 2)), 1), c.id, d.device_os_version
			) ta
	group by pp_date, device_os_version, id) tb
group by pp_date, device_os_version




select p_date, count(distinct id)
from muce_topic.applicationstart
where p_date >= 20151226 and p_date <= 20151228 and p_product = 'ANDROID_EYEPETIZER' and is_start_new = 1 and client_first_source = 'eyepetizer_xiaomi_market'
group by p_date

select p_date, sum(case when p_product = 'IOS_EYEPETIZER' then 1 else 0 end), sum(case when p_product = 'ANDROID_EYEPETIZER' then 1 else 0 end)
from muce_topic.click
where p_date >= 20160102 and p_date <= 20160103 and click_name = 'Play'
group by p_date



and click_name in ('ShareCoverMoment','ShareCoverWeibo','Facebook','ShareCoverQQ','Weibc','More','Moment','ShareCoverWechat','Weibo','ShareCoverTwitter','Twitter','ShareCoverFacebook','Wechat','Link','ShareCoverMore','QQ','ShareCoverWeibc')






select a.id, b.device_model
from (
	select id
	from muce_app_client.taskevent
	where p_product= 'ANDROID_EYEPETIZER' and p_date = 20151120 and task_action = "PLAY" and task_status='END' and content_id = 'player_error') a
join (
	select id, device_model
	from muce_app_client.applicationactiveevent
	where p_product= 'ANDROID_EYEPETIZER' and p_date = 20151120) b
on a.id = b.id



select content_id, sum(case when p_product = 'IOS_EYEPETIZER' then 1 else 0 end), sum(case when p_product = 'ANDROID_EYEPETIZER' then 1 else 0 end)
from muce_topic.click
where p_date = 20160103 and click_name = 'Play'
group by content_id



select click_normalized_url, count(*) as COUNT
from muce_topic.click
where p_product = 'RIPPLE' and p_date >= '20151228' and p_date <= '20160103' and click_name = 'share'
group by  click_normalized_url
order by COUNT DESC LIMIT 100

select aaa, sum(uv_num), sum(play_id_num), sum(share_id_num), sum(fav_id_num) from ( select content_id as aaa, count(distinct id) as uv_num, '0' as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_app_client.showevent where p_product= 'IOS_EYEPETIZER' and p_date= 20150106 and content_id IN (4548,4398,4052,3546,4066) group by content_id union all select content_id as aaa, '0' as uv_num, count(distinct id) as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_app_client.clickevent where p_product='IOS_EYEPETIZER' and click_name='Play' and p_date= 20150106 and content_id IN (4548,4398,4052,3546,4066) group by content_id union all select case when content_id > 0 then content_id else substring(content_id, 10, length(content_id) - 10) end as aaa, '0' as uv_num, '0' as play_id_num, count(distinct id) as share_id_num, '0' as fav_id_num from muce_app_client.clickevent where p_product = 'IOS_EYEPETIZER' and click_action = 'SHARE' and click_name in ('ShareCoverMoment','ShareCoverWeibo','Facebook','ShareCoverQQ','Weibc','More','Moment','ShareCoverWechat','Weibo','ShareCoverTwitter','Twitter','ShareCoverFacebook','Wechat','Link','ShareCoverMore','QQ','ShareCoverWeibc') and p_date= 20150106 and (content_id like '%4548%' or content_id like '%4398%' or content_id like '%4052%' or content_id like '%3546%' or content_id like '%4066%') group by case when content_id > 0 then content_id else substring(content_id, 10, length(content_id) - 10) end union all select content_id as aaa, '0' as uv_num, '0' as play_id_num, '0' as share_id_num, count(distinct id) as fav_id_num from muce_app_client.clickevent where p_product = 'IOS_EYEPETIZER' and click_name = 'Favorite' and p_date= 20150106 and content_id IN (4548,4398,4052,3546,4066) group by content_id) a group by aaa --iOS 20150106

select aaa, sum(uv_num), sum(play_id_num), sum(share_id_num), sum(fav_id_num) from ( select content_id as aaa, count(distinct id) as uv_num, '0' as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_app_client.showevent where p_product= 'IOS_EYEPETIZER' and p_date= 20150106 and content_id IN (4548,4398,4052,3546,4066) group by content_id union all select content_id as aaa, '0' as uv_num, count(distinct id) as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_app_client.clickevent where p_product='IOS_EYEPETIZER' and click_name='Play' and p_date= 20150106 and content_id IN (4548,4398,4052,3546,4066) group by content_id union all select case when content_id > 0 then content_id else substring(content_id, 10, length(content_id) - 10) end as aaa, '0' as uv_num, '0' as play_id_num, count(distinct id) as share_id_num, '0' as fav_id_num from muce_app_client.clickevent where p_product = 'IOS_EYEPETIZER' and click_action = 'SHARE' and click_name in ('ShareCoverMoment','ShareCoverWeibo','Facebook','ShareCoverQQ','Weibc','More','Moment','ShareCoverWechat','Weibo','ShareCoverTwitter','Twitter','ShareCoverFacebook','Wechat','Link','ShareCoverMore','QQ','ShareCoverWeibc') and p_date= 20150106 and (content_id like '%4548%' or content_id like '%4398%' or content_id like '%4052%' or content_id like '%3546%' or content_id like '%4066%') group by case when content_id > 0 then content_id else substring(content_id, 10, length(content_id) - 10) end union all select content_id as aaa, '0' as uv_num, '0' as play_id_num, '0' as share_id_num, count(distinct id) as fav_id_num from muce_app_client.clickevent where p_product = 'IOS_EYEPETIZER' and click_name = 'Favorite' and p_date= 20150106 and content_id IN (4548,4398,4052,3546,4066) group by content_id) a group by aaa --iOS 20150106





