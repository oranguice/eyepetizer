select id, content_id, '', sum(case when click_name = 'Play' then 1 else 0 end), sum(case when click_action = 'SHARE' and click_name in ('Moment','Wechat','QQ','Weibo','SystemShare') then 1 else 0 end)
from muce_topic.click
where p_date >= 20151201 and p_date <= 20160111 and p_product = 'ANDROID_EYEPETIZER'
group by id, content_id


select content_id, sum(uv_num), sum(play_id_num), sum(share_id_num), sum(fav_id_num) from ( select content_id, count(distinct id) as uv_num, '0' as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_topic.show where p_product= 'ANDROID_EYEPETIZER'  and p_date = 20160108 and content_id in (4036,3976,4650,4558,4058) group by content_id union all select content_id, '0' as uv_num, count(distinct id) as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_topic.click where p_product='ANDROID_EYEPETIZER'  and p_date = 20160108 and content_id in (4036,3976,4650,4558,4058) and click_name = 'Play' group by content_id union all select content_id, '0' as uv_num, '0' as play_id_num, count(distinct id) as share_id_num, '0' as fav_id_num from muce_topic.click where p_product = 'ANDROID_EYEPETIZER' and click_action = 'SHARE' and click_name in ('Moment','Wechat','QQ','Weibo','SystemShare') and p_date = 20160108 and content_id in (4036,3976,4650,4558,4058) group by content_id union all        select content_id, '0' as uv_num, '0' as play_id_num, '0' as share_id_num, count(distinct id) as fav_id_num from muce_topic.click where p_product = 'ANDROID_EYEPETIZER' and click_name = 'Favorite' and p_date = 20160108 and content_id in (4036,3976,4650,4558,4058) group by content_id) a group by content_id --Android 20160108



select id, case when length(content_id) < 6 then content_id else null end, '', sum(case when click_name = 'Play' then 1 else 0 end), sum(case when click_action = 'SHARE' and click_name in ('Moment','Wechat','QQ','Weibo','SystemShare') then 1 else 0 end)
from muce_topic.click
where p_date >= 20151201 and p_date <= 20160111 and p_product = 'ANDROID_EYEPETIZER'
group by id, case when length(content_id) < 6 then content_id else null end




