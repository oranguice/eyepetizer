	sum(case when p_product = 'IOS_EYEPETIZER' and click_name='Play' then 1 else 0 end),
	sum(case when p_product = 'ANDROID_EYEPETIZER' and click_name = 'Play' then 1 else 0 end),
	sum(case when p_product = 'IOS_EYEPETIZER' and click_action = 'SHARE' and click_name in ('ShareCoverMoment','ShareCoverWeibo','Facebook','ShareCoverQQ','Weibc','More','Moment','ShareCoverWechat','Weibo','ShareCoverTwitter','Twitter','ShareCoverFacebook','Wechat','Link','ShareCoverMore','QQ','ShareCoverWeibc') then 1 else 0 end),
	sum(case when p_product = 'ANDROID_EYEPETIZER' and click_action = 'SHARE' and click_name in ('Moment','Wechat','QQ','Weibo','SystemShare') then 1 else 0 end)
from muce_app_client.clickevent
where p_date = 20160302
group by p_date
--播放与分享次数


select aaa, sum(uv_num), sum(play_id_num), sum(share_id_num), sum(fav_id_num) from ( select content_id as aaa, count(*) as uv_num, '0' as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_app_client.showevent where p_product= 'IOS_EYEPETIZER' and p_date= 20160302 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id IN (3844,3986,4724,5368,5782) group by content_id union all select content_id as aaa, '0' as uv_num, count(*) as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_app_client.clickevent where p_product='IOS_EYEPETIZER' and click_name='Play' and p_date= 20160302 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id IN (3844,3986,4724,5368,5782) group by content_id union all select case when content_id > 0 then content_id else substring(content_id, 10, length(content_id) - 10) end as aaa, '0' as uv_num, '0' as play_id_num, count(*) as share_id_num, '0' as fav_id_num from muce_app_client.clickevent where p_product = 'IOS_EYEPETIZER' and click_action = 'SHARE' and click_name in ('ShareCoverMoment','ShareCoverWeibo','Facebook','ShareCoverQQ','Weibc','More','Moment','ShareCoverWechat','Weibo','ShareCoverTwitter','Twitter','ShareCoverFacebook','Wechat','Link','ShareCoverMore','QQ','ShareCoverWeibc') and p_date= 20160302 and p_hourmin >= 0000 and p_hourmin <= 1800 and (content_id like '%3844%' or content_id like '%3986%' or content_id like '%4724%' or content_id like '%5368%' or content_id like '%5782%') group by case when content_id > 0 then content_id else substring(content_id, 10, length(content_id) - 10) end union all select content_id as aaa, '0' as uv_num, '0' as play_id_num, '0' as share_id_num, count(*) as fav_id_num from muce_app_client.clickevent where p_product = 'IOS_EYEPETIZER' and click_name = 'Favorite' and p_date= 20160302 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id IN (3844,3986,4724,5368,5782) group by content_id) a group by aaa --iOS 20160302



select content_id, sum(uv_num), sum(play_id_num), sum(share_id_num), sum(fav_id_num) from ( select content_id, count(*) as uv_num, '0' as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_app_client.showevent where p_product= 'ANDROID_EYEPETIZER'  and p_date = 20160302 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id in (3844,3986,4724,5368,5782) group by content_id union all select content_id, '0' as uv_num, count(*) as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_app_client.clickevent where p_product='ANDROID_EYEPETIZER'  and p_date = 20160302 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id in (3844,3986,4724,5368,5782) and click_name = 'Play' group by content_id union all select content_id, '0' as uv_num, '0' as play_id_num, count(*) as share_id_num, '0' as fav_id_num from muce_app_client.clickevent where p_product = 'ANDROID_EYEPETIZER' and click_action = 'SHARE' and click_name in ('Moment','Wechat','QQ','Weibo','SystemShare') and p_date = 20160302 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id in (3844,3986,4724,5368,5782) group by content_id union all select content_id, '0' as uv_num, '0' as play_id_num, '0' as share_id_num, count(*) as fav_id_num from muce_app_client.clickevent where p_product = 'ANDROID_EYEPETIZER' and click_name = 'Favorite' and p_date = 20160302 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id in (3844,3986,4724,5368,5782) group by content_id) a group by content_id --Android 20160302



