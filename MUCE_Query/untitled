from muce_topic.click
where p_date >= 20160301 and p_date <= 20160307 and click_name in ('创意','运动','旅行','剧情','动画','广告','音乐','开胃','预告','综合','记录','时尚') and p_product = 'IOS_EYEPETIZER'
group by click_name


