SET @row=0;
SET @mid='';
SELECT a.*,b.rownum FROM test a 
INNER JOIN (
SELECT cid, author, id, CASE WHEN @mid = author THEN @row:=@row+1 ELSE @row:=1 END rownum, @mid:=author MID 
FROM test 
ORDER BY cid,author,id DESC
) b ON b.author=a.author AND b.cid=a.cid AND b.id=a.id  WHERE b.rownum<3;  




select a.udid, a.category, max(a.num)
from (
	select udid, category, count(*) as num
	from ios_play_share
	where play_times > 0
	group by udid, category
	) a
left outer join (
	select udid, category, max(m) as max_num
	from (
		select udid, category, count(*) as m
		from ios_play_share
		where play_times > 0
		group by udid, category) b
	) c
on a.udid = c.udid and a.category = c.category and a.num = c.max_num
where c.max_num is null


select a.udid, a.category, a.m
from (
	select udid, category, count(*) as m
	from ios_play_share
	where play_times > 0
	group by udid, category) a
join (
	select udid, max(m) as max_num
	from (
		select udid, category, count(*) as m
		from ios_play_share
		where play_times > 0
		group by udid, category) b
	group by udid) c
on a.udid = c.udid and a.m = c.max_num




select @row_number := CASE
	when @paiming = udid then @row_number + 1 else 1
	end as num,
	@paiming := udid as udid
from ios_play_share
limit 100



select pp_date, sum(case when col_flag = 1 then col_num else 0 end), sum(case when col_flag = 2 then col_num else 0 end), sum(case when col_flag = 3 then col_num else 0 end)
from (
	select pp_date, col_flag, count(*) as col_num
		from
		(
			select pp_date, id, sum(col_flag) as col_flag 
			from 
			( 
					select concat(substring(p_date, 1, 4), '-', substring(p_date, 5, 2), '-', substring(p_date, 7, 2)) as pp_date, id, 1 as col_flag 
						from muce_topic.applicationstart
						where is_start_new = 0 and p_date >= 20160101 and p_date <= 20160112 and p_product = 'IOS_EYEPETIZER' and substring(id, 40, 1) in ('0','1','2','3')
					group by concat(substring(p_date, 1, 4), '-', substring(p_date, 5, 2), '-', substring(p_date, 7, 2)), id
				union all 
					select date_sub(concat(substring(p_date, 1, 4), '-', substring(p_date, 5, 2), '-', substring(p_date, 7, 2)), 1) as pp_date, id, 2 as col_flag 
						from muce_topic.applicationstart
						where p_date >= 20160102 and p_date <= 20160113 and p_product = 'IOS_EYEPETIZER'
					group by date_sub(concat(substring(p_date, 1, 4), '-', substring(p_date, 5, 2), '-', substring(p_date, 7, 2)), 1), id
			)ta 
			group by pp_date, id
		)tb
	group by pp_date, col_flag )tc
group by pp_date
