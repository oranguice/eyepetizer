select id, click_action, click_name, click_element, content_title, is_start_new
from muce_topic.click
where p_date = 20160125 and p_product = 'ANDROID_EYEPETIZER' and click_url = 'autoDownload/userGuide'



select p_date, sum(case when task_result_info like 'autoDownload%' then 1 else 0 end), sum(case when task_result_info like 'autoDownload:true%' then 1 else 0 end), sum(case when task_result_info like '%enableNotification:true%' then 1 else 0 end), sum(case when task_result_info like '%2g3g:true' then 1 else 0 end)
from muce_topic.task
where p_date >= 20160120 and p_date <= 20160125 and p_product = 'ANDROID_EYEPETIZER' and task_result_info like 'autoDownload%'
group by p_date





select a.id
from (
	select distinct id
	from muce_topic.click
	where p_date = 20160126 and p_product = 'IOS_EYEPETIZER' and click_name like 'FeedPageEventClickhttp://wandou.im/1b3ze8') a join (
	select distinct id
	from muce_topic.click
	where p_date = 20160126 and p_product = 'IOS_EYEPETIZER' and click_action = 'SHARE' and click_name = 'Moment' ) b
on a.id = b.id




select a.id
from (
	select distinct id
	from muce_topic.click
	where p_date = 20160126 and p_product = 'ANDROID_EYEPETIZER' and card_id = 28) a
join (
	select distinct id
	from muce_topic.click
	where p_date = 20160126 and p_product = 'ANDROID_EYEPETIZER' and click_action = 'SHARE' and click_name = 'Moment') b
on a.id = b.id


select p_date, content_id, sum(case when p_product = 'IOS_EYEPETIZER' then 1 else 0 end), sum(case when p_product = 'ANDROID_EYEPETIZER' then 1 else 0 end)
from muce_topic.show
where p_date >= 20160123 and p_date <= 20160126  and content_id in('5002')
group by p_date, content_id


select p_date, sum(case when p_product= 'IOS_EYEPETIZER' then 1 else 0 end), sum(case when p_product= 'ANDROID_EYEPETIZER' then 1 else 0 end)
from muce_topic.click
where p_date >= 20160123 and p_date <= 20160126 and content_id in ('5002') and click_name = 'Play'
group by p_date


select p_date, count(*)
from muce_topic.click
where p_product = 'ANDROID_EYEPETIZER' and click_action = 'SHARE' and click_name in ('Moment','Wechat','QQ','Weibo','SystemShare') and p_date >= 20160123 and p_date <= 20160126 and content_id = '5002'
group by p_date



