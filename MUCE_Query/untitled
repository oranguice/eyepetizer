group by p_date, p_hourmin



select p_date, count(*)
from muce_app_client.applicationstartevent
where p_date >= 20160109 and p_date <= 20160110 and p_product = 'IOS_EYEPETIZER' and (p_hourmin = '0900' or p_hourmin = '1200')
group by p_date


select p_date, p_hourmin, count(distinct case when p_product = 'IOS_EYEPETIZER' then id else null end), sum(case when p_product = 'IOS_EYEPETIZER' then 1 else 0 end), count(distinct case when p_product = 'ANDROID_EYEPETIZER' then id else null end), sum(case when p_product = 'ANDROID_EYEPETIZER' then 1 else 0 end)
from muce_app_client.clickevent
where p_date >= 20160101 and p_date <= 20160117 and click_name = 'Play'
group by p_date, p_hourmin



