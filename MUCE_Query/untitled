#开眼# 星战正在全球引起热潮，美国著名综艺「周六夜现场」在“新节目 - 卧底老板”中邀请到了星球大战第一军团的 BOSS 凯洛伦。本次节目他将伪装成雷达技师马特进入弑星者基地，体察普通员工的日常工作与生活。老板会有什么新发现呢？更多内容：O网页链接 播放： O秒拍视频





select 
 extra_package.timing_package.start as timing_start,
 extra_package.timing_package.`end` as timing_end,
 extra_package.timing_package.duration as timing_duration,
 event_package.task_event.result as task_result 
from muce_event_client.taskevent 
where 
 p_product= 'ANDROID_EYEPETIZER' and 
 p_date >= 20160118 and 
 common_package.client_package.version_code >= 70 and 
 event_package.task_event.action = "PLAY" and 
 event_package.task_event.status='PENDING' and
 common_package.id_package.identity like "%9"
sort by timing_start, timing_duration





select p_date, count(distinct id)
from muce_topic.applicationstart
where p_date >= 20160101 and p_date <= 20160119 and p_product = 'ANDROID_EYEPETIZER' and client_first_source = 'eyepetizer_xiaomi_market' and is_start_new = 1
group by p_date




select pp_date, sum(case when col_flag = 1 then col_num else 0 end), sum(case when col_flag = 2 then col_num else 0 end), sum(case when col_flag = 3 then col_num else 0 end)
from (
	select pp_date, col_flag, count(*) as col_num
		from
		(
			select pp_date, id, sum(col_flag) as col_flag 
			from 
			( 
					select concat(substring(p_date, 1, 4), '-', substring(p_date, 5, 2), '-', substring(p_date, 7, 2)) as pp_date, id, 1 as col_flag 
						from muce_topic.applicationstart
						where is_start_new = 1 and p_date >= 20160101 and p_date <= 20160118 and p_product = 'ANDROID_EYEPETIZER' and client_first_source = 'eyepetizer_zhihuiyun_market'
					group by concat(substring(p_date, 1, 4), '-', substring(p_date, 5, 2), '-', substring(p_date, 7, 2)), id
				union all 
					select date_sub(concat(substring(p_date, 1, 4), '-', substring(p_date, 5, 2), '-', substring(p_date, 7, 2)), 1) as pp_date, id, 2 as col_flag 
						from muce_topic.applicationstart
						where p_date >= 20160102 and p_date <= 20160119 and p_product = 'ANDROID_EYEPETIZER'
					group by date_sub(concat(substring(p_date, 1, 4), '-', substring(p_date, 5, 2), '-', substring(p_date, 7, 2)), 1), id
			)ta 
			group by pp_date, id
		)tb
	group by pp_date, col_flag )tc
group by pp_date
--Android360渠道新用户次日活跃

select count(distinct case when p_product = 'IOS_EYEPETIZER' then id else null end), count(distinct case when p_product = 'ANDROID_EYEPETIZER' then id else null end)
from muce_topic.applicationstart
where p_date >= 20151221 and p_date <= 20160119


