select aaa, sum(uv_num), sum(play_id_num), sum(share_id_num), sum(fav_id_num) from ( select content_id as aaa, count(distinct id) as uv_num, '0' as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_app_client.showevent where p_product= 'IOS_EYEPETIZER' and p_date= 20160201 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id IN (4718,5198,4344,5202,5156) group by content_id union all select content_id as aaa, '0' as uv_num, count(distinct id) as play_id_num, '0' as share_id_num, '0' as fav_id_num from muce_app_client.clickevent where p_product='IOS_EYEPETIZER' and click_name='Play' and p_date= 20160201 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id IN (4718,5198,4344,5202,5156) group by content_id union all select case when content_id > 0 then content_id else substring(content_id, 10, length(content_id) - 10) end as aaa, '0' as uv_num, '0' as play_id_num, count(distinct id) as share_id_num, '0' as fav_id_num from muce_app_client.clickevent where p_product = 'IOS_EYEPETIZER' and click_action = 'SHARE' and click_name in ('ShareCoverMoment','ShareCoverWeibo','Facebook','ShareCoverQQ','Weibc','More','Moment','ShareCoverWechat','Weibo','ShareCoverTwitter','Twitter','ShareCoverFacebook','Wechat','Link','ShareCoverMore','QQ','ShareCoverWeibc') and p_date= 20160201 and p_hourmin >= 0000 and p_hourmin <= 1800 and (content_id like '%4718%' or content_id like '%5198%' or content_id like '%4344%' or content_id like '%5202%' or content_id like '%5156%') group by case when content_id > 0 then content_id else substring(content_id, 10, length(content_id) - 10) end union all select content_id as aaa, '0' as uv_num, '0' as play_id_num, '0' as share_id_num, count(distinct id) as fav_id_num from muce_app_client.clickevent where p_product = 'IOS_EYEPETIZER' and click_name = 'Favorite' and p_date= 20160201 and p_hourmin >= 0000 and p_hourmin <= 1800 and content_id IN (4718,5198,4344,5202,5156) group by content_id) a group by aaa --iOS 20160201



