select p_date, count(distinct id)
from muce_topic.applicationstart
where p_date >= 20160307 and p_date <= 20160310 and p_product = 'IOS_EYEPETIZER'
group by p_date


select device_os_version, device_model
from muce_topic.applicationactive
where p_date = 20160307 and p_product = 'ANDROID_EYEPETIZER'
limit 100