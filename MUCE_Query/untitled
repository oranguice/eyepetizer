select click_name, count(*)
from muce_topic.click
where p_date >= 20160301 and p_date <= 20160307 and p_product = 'IOS_EYEPETIZER' and click_name in ('创意','运动','旅行','剧情','动画','广告','音乐','开胃','预告','综合','记录','时尚')
group by click_name



select p_date, click_url, click_name, count(*)
from muce_topic.click
where p_date >= 20160301 and p_date <= 20160307 and click_url like 'common/api/v3/videoscategoryName%' and click_url like '%date' and click_url not like 'SwitchTab%'
group by p_date, click_url, click_name


select p_date, click

\N
ViewCache
SwitchTab_2
SwitchTab_0
ViewFavorites
SwitchTab_1
ViewReply
ViewFeeback
Play



select p_date, click_name, count(*)
from muce_topic.click
where p_date = 20160308 and p_product = 'IOS_EYEPETIZER' and click_url = '/videolist?type=feed' and click_name not in (null, 'ViewCache', 'SwitchTab_2', 'SwitchTab_0', 'ViewFavorites', 'SwitchTab_1', 'ViewReply', 'ViewFeeback', 'Play', 'Feed')
group by p_date, click_name



select p_date, click_name, count(*)
from muce_topic.click
where p_date = 20160308 and p_product = 'ANDROID_EYEPETIZER' and click_url = 'feed' and click_name not in (null, 'ViewCache', 'SwitchTab_2', 'SwitchTab_0', 'ViewFavorites', 'SwitchTab_1', 'ViewReply', 'ViewFeeback', 'Play', 'Feed')
group by p_date, click_name


