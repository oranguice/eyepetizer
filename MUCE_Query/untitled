from muce_topic.click
where p_date = 20160126 and p_product = 'IOS_EYEPETIZER' and click_name like 'FeedPageEventClickhttp://wandou.im/1b3ze8'


select count(distinct id)
from muce_topic.click
where p_date = 20160126 and p_product = 'ANDROID_EYEPETIZER' and card_id = 28

select a.id
from (
	select distinct id
	from muce_topic.applicationstart
	where p_date >= 20151101 and p_date <= 20160126 and p_product = 'ANDROID_EYEPETIZER') a
left outer join (
	select distinct id
	from muce_topic.click
	where p_date >= 20160125 and p_date <= 20160126 and p_product = 'ANDROID_EYEPETIZER' and card_id = 28) b
on a.id = b.id
where b.id is null



