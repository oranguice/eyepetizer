StringBuilder stringBuilder = new StringBuilder();
stringBuilder.append("autoDownload:")
.append(Config.get(Const.SharedPreferences.ENABLE_AUTO_DOWNLOAD, false))
.append("enableNotification:")
.append(Config.get(Const.SharedPreferences.ENABLE_DAILY_PUSH_NOTIFICATION, true))
.append("alert2g3g:")
.append(Config.get(Const.SharedPreferences.ENABLE_CELLULAR_NOTIFICATION, true));

TaskEvent.Builder builder = new TaskEvent.Builder();
builder.action(TaskEvent.Action.USER_STAT).status(TaskEvent.Status.READY)
.result(TaskEvent.Result.SUCCESS).result_info(stringBuilder.toString());




click=ViewLogPackage{action=CLOSE, element=BUTTON, name=暂不开启, url_package=UrlPackage{normalized_url=autoDownload/userGuide, url=autoDownload/userGuide}}, from_click=ViewLogPackage{}, refer_click=ViewLogPackage{}}}
