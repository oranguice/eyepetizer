select a.id
from (
	select id
	from muce_topic.applicationstart
	where p_date >= 20160201 and p_date <= 20160306 and p_product = 'IOS_EYEPETIZER'
	group by id) a
left outer join (
	select id
	from muce_app_client.applicationstartevent
	where p_date >= 20160305 and p_date <= 20160307 and p_product = 'IOS_EYEPETIZER'
	group by id) b
on a.id = b.id
where b.id is null


