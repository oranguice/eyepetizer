final ContentPackage.Builder contentBuilder = new ContentPackage.Builder();
   contentBuilder
       .identity(videoModel == null ? "unknown" : String.valueOf(videoModel.getModelId()))
       .parent_id(String.valueOf(parentId))
       .title(videoModel == null ? "unknown" : videoModel.getTitle());

   final TimingPackage.Builder timingBuilder = new TimingPackage.Builder();
   timingBuilder.start(sectionStartTime)
       .end(current).duration(coast);

   ExtraPackage.Builder extraBuilder = new ExtraPackage.Builder();
   extraBuilder.content_package(contentBuilder.build())
       .timing_package(timingBuilder.build());

   TaskEvent.Builder taskBuilder = new TaskEvent.Builder();
   taskBuilder.action(TaskEvent.Action.PLAY)
       .status(TaskEvent.Status.END)
       .result(TaskEvent.Result.SUCCESS).result_info("timing1");

   LogManager.getLogger().logTaskEvent(taskBuilder, extraBuilder);


