select b.p_date, count(distinct b.id), sum(b.num)
from (
	select p_date, id
	from muce_topic.click
	where p_date >= 20151217 and p_date <= 20151230 and click_name = 'Comment' and p_product = 'IOS_EYEPETIZER'
	group by p_date, id) a
join (
	select p_date, id, count(*) as num
	from muce_topic.click
	where p_date >= 20151217 and p_date <= 20151230 and click_action = 'SHARE' and click_name in ('ShareCoverMoment','ShareCoverWeibo','Facebook','ShareCoverQQ','Weibc','More','Moment','ShareCoverWechat','Weibo','ShareCoverTwitter','Twitter','ShareCoverFacebook','Wechat','Link','ShareCoverMore','QQ','ShareCoverWeibc') and p_product = 'IOS_EYEPETIZER'
	group by p_date, id) b
on a.p_date = b.p_date and a.id = b.id
group by b.p_date





